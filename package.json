{
  "name": "hackerzen",
  "version": "0.1.0",
  "description": "Minimalist Hacker News",
  "keywords": [
    "Hacker News",
    "YCombinator",
    "Tech"
  ],
  "homepage": "https://github.com/proc0/HackerZen#readme",
  "bugs": {
    "url": "https://github.com/proc0/HackerZen/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proc0/HackerZen.git"
  },
  "license": "MIT",
  "author": "proc0",
  "type": "module",
  "source": "script/main.js",
  "outputFormat": "global",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "git subtree push --prefix dist origin gh-pages",
    "build": "npx parcel build index.html",
    "bundle": "node concat-js dist dist/bundle.js"
  },
  "targets": {
    "default": {
      "includeNodeModules": false,
      "publicUrl": ".",
      "sourceMap": false
    }
  },
  "dependencies": {
    "@koa/bodyparser": "^6.0.0",
    "@koa/router": "^14.0.0",
    "koa": "^3.0.3",
    "node-html-parser": "^7.0.1"
  },
  "devDependencies": {
    "parcel": "^2.16.1"
  }
}
